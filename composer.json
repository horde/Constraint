{
    "name": "horde/constraint",
    "description": "Constraint library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Constraint",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        }
    ],
    "time": "2022-11-05",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8 || ^9"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0"
    },
    "suggest": {},
    "autoload": {
        "psr-0": {
            "Horde_Constraint": "lib/"
        },
        "psr-4": {
            "Horde\\Constraint\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Constraint\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}